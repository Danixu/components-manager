{
	"op_amp": {
		"name": "Amplificador Operacional",
		"default_name": "%(model_number): %(voltage_vin) - %(circuit_number) - %(slew_rate) - (%(package_type))",
		"data": {
			"model": {
				"text": "Modelo",
				"controls": {
					"number": {
						"type": "Input",
						"placeholder": "Modelo del componente",
						"required": true
					}
				}
			},
			"package": {
				"text": "Paquete",
				"controls": {
					"type": {
						"type": "ComboBox",
						"from_values": "packages"
					}
				}
			},
			"type": {
				"text": "Tipo",
				"controls": {
					"value": {
						"type": "ComboBox",
						"choices": [
							"Propósito General"
						]
					}
				}
			},
			"circuit": {
				"text": "Nº Circuitos",
				"controls": {
					"number": {
						"type": "ComboBox",
						"default": "Double (2)",
						"choices": [
							"Single (1)",
							"Double (2)",
							"Quad (4)"
						]
					}
				}
			},
			"voltage": {
				"text": "Voltaje de entrada",
				"controls": {
					"vin": {
						"type": "Input",
						"placeholder": "Rango de voltaje de funcionamiento"
					}
				}
			},
			"iqpc": {
				"text": "Iq por canal",
				"controls": {
					"current": {
						"type": "Input",
						"placeholder": "Consumo de corriente en espera por canal"
					},
					"current_multiplier": {
						"type": "ComboBox",
						"size": 90,
						"sort": false,
						"nospace": true,
						"default": "mA (10^−3)",
						"from_values": "current_multipliers"
					}
				}
			},
			"gbp": {
				"text": "Gain Bandwidth Product (GBP)",
				"controls": {
					"value": {
						"type": "Input",
						"placeholder": "Ancho de banda de ganancia"
					},
					"multiplier": {
						"type": "ComboBox",
						"nospace": true,
						"from_values": "frecuencies_multipliers"
					}
				}
			},
			"slew": {
				"text": "Slew Rate (SR)",
				"controls": {
					"rate": {
						"type": "Input",
						"placeholder": "Velocidad de bajada/subida de voltaje"
					}
				}
			},
			"recycled": {
				"text": "Reciclada",
				"controls": {
					"recycled": {
						"type": "CheckBox",
						"default": "false"
					}
				}
			}
		}
	}
}