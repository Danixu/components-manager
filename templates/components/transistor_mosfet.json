{
	"mosfet_transistor": {
		"name": "Transistor Mosfet",
		"default_name": "Mosfet %(model_number): %(type_value) - %(vdss_voltage)%(vdss_volt_factor) - %(iqpc_current)%(iqpc_current_multiplier) - %(rds_resistance)%(rds_factor)@%(rds_voltage)%(rds_volt_factor) ",
		"data": {
			"model": {
				"text": "Modelo",
				"controls": {
					"number": {
						"type": "Input",
						"placeholder": "Modelo del componente",
						"required": true
					}
				}
			},
			"package": {
				"text": "Paquete",
				"controls": {
					"type": {
						"type": "ComboBox",
						"from_values": "packages_ic"
					}
				}
			},
			"type": {
				"text": "Tipo Unión",
				"controls": {
					"value": {
						"type": "ComboBox",
						"choices": [
							"PNP",
							"NPN"
						]
					}
				}
			},
			"vdss": {
				"text": "Vdss",
				"controls": {
					"voltage": {
						"type": "Input",
						"placeholder": "Voltaje Drain2Source"
					},
					"volt_factor": {
						"type": "ComboBox",
						"size": 90,
						"sort": false,
						"nospace": true,
						"default": "V",
						"from_values": "voltage_multipliers"
					}
				}
			},
			"iqpc": {
				"text": "Id",
				"controls": {
					"current": {
						"type": "Input",
						"placeholder": "Corriente drenaje continuo"
					},
					"current_multiplier": {
						"type": "ComboBox",
						"size": 90,
						"sort": false,
						"nospace": true,
						"default": "mA",
						"from_values": "current_multipliers"
					}
				}
			},
			"vgs": {
				"text": "Vgs(th) (Max)",
				"controls": {
					"voltage": {
						"type": "Input",
						"placeholder": "Voltaje Puerta"
					},
					"volt_factor": {
						"type": "ComboBox",
						"size": 50,
						"sort": false,
						"nospace": true,
						"default": "V",
						"from_values": "voltage_multipliers"
					},
					"current": {
						"type": "Input",
						"label": "@",
						"placeholder": "Corriente puerta",
						"required": true
					},
					"current_multiplier": {
						"type": "ComboBox",
						"size": 50,
						"sort": false,
						"nospace": true,
						"default": "uA",
						"from_values": "current_multipliers"
					}
				}
			},
			"rds": {
				"text": "Rds ON (Max)",
				"controls": {
					"resistance": {
						"type": "Input",
						"placeholder": "Resistencia Drain to Source On",
						"required": true
					},
					"factor": {
						"type": "ComboBox",
						"size": 50,
						"sort": false,
						"nospace": true,
						"default": "Ω",
						"from_values": "resistance_factor"
					},
					"voltage": {
						"type": "Input",
						"label": "@",
						"placeholder": "Voltaje Puerta"
					},
					"volt_factor": {
						"type": "ComboBox",
						"size": 50,
						"sort": false,
						"nospace": true,
						"default": "V",
						"from_values": "voltage_multipliers"
					}
				}
			},
			"power_disipation": {
				"text": "Disip. máxima",
				"controls": {
					"current": {
						"type": "Input",
						"placeholder": "Potencia"
					},
					"current_multiplier": {
						"type": "ComboBox",
						"size": 90,
						"sort": false,
						"nospace": true,
						"default": "W",
						"from_values": "power_multipliers"
					}
				}
			}
		}
	}
}